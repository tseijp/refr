{
        "name": "refr",
        "version": "0.7.0",
        "author": "tseijp",
        "license": "MIT",
        "private": false,
        "sideEffect": false,
        "publishConfig": {
                "access": "public"
        },
        "bugs": {
                "url": "https://github.com/tseijp/refr/issues"
        },
        "repository": "tseijp/refr",
        "homepage": "https://refr.tsei.jp",
        "keywords": [
                "animation",
                "frame",
                "hooks",
                "raf",
                "react",
                "reactjs",
                "reactive",
                "requestAnimationFrameanimation",
                "solid",
                "tiny",
                "typescript"
        ],
        "files": [
                "dist/**/*.{js,ts,mjs,mts,map}",
                "packages.json",
                "README.md"
        ],
        "main": "./dist/index.js",
        "types": "./dist/index.d.ts",
        "module": "./dist/index.mjs",
        "exports": {
                "./packages.json": "./packages.json",
                ".": {
                        "types": "./dist/index.d.ts",
                        "import": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.mjs"
                        },
                        "module": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.mjs"
                        },
                        "require": "./dist/index.js",
                        "default": "./dist/index.js"
                },
                "./*": {
                        "types": "./dist/index.d.ts",
                        "import": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.mjs"
                        },
                        "module": {
                                "types": "./dist/index.d.ts",
                                "default": "./dist/index.mjs"
                        },
                        "require": "./dist/index.js",
                        "default": "./dist/index.js"
                },
                "./react": {
                        "types": "./dist/react.d.ts",
                        "import": {
                                "types": "./dist/react.d.ts",
                                "default": "./dist/react.mjs"
                        },
                        "module": {
                                "types": "./dist/react.d.ts",
                                "default": "./dist/react.mjs"
                        },
                        "require": "./dist/react.js",
                        "default": "./dist/react.js"
                },
                "./react/*": {
                        "types": "./dist/react.d.ts",
                        "import": {
                                "types": "./dist/react.d.ts",
                                "default": "./dist/react.mjs"
                        },
                        "module": {
                                "types": "./dist/react.d.ts",
                                "default": "./dist/react.mjs"
                        },
                        "require": "./dist/react.js",
                        "default": "./dist/react.js"
                }
        }
}
